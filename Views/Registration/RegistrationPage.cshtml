    <script type="text/javascript">
        $(document).on("click", "#btn_submit", function () {
            var loader = document.getElementById("overlay");
            loader.style.display = "block";
            var uname = $("#txt_Name").val();
            var phn = $("#txt_Mobile").val();
            var email = $("#txt_email").val();
            var pass = $("#txt_Password").val();
            var txtSearchByName = $("#txtSearchByName").val();
            if ($("#isSelected").is(':checked')) {
                if (window.FormData !== undefined) {
                    var fdata = new FormData();
                    fdata.append("_uname", uname);
                    fdata.append("_phn", phn);
                    fdata.append("_email", email);
                    fdata.append("_pass", pass);
                    fdata.append("_advisor", txtSearchByName);
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("UserRegistration","Registration")',
                        contentType: false,
                        processData: false,
                        data: fdata,
                        dataType: "Json",
                        success: function (data) {
                            if (data == "Success") {
                                loader.style.display = "none";
                                swal("Thank you for signing up, kindly check your email/sms for completing the authentication process required for login in.");
                                setTimeout(function () {
                                    document.location = "@Url.Content("~/Login/LoginPage")";
                                }, 2000);
                            }
                            else {
                                loader.style.display = "none";
                            }
                        },
                        error: function () {

                        }
                    })
                }
            }
            else {
                loader.style.display = "none";
                    swal("unchecked");
                }
            })
    </script>

