<!--Swp Fund and Send Data-->
<script type="text/javascript">
    $('body').on('click', '#btn_buybtnSwp', function (e) {
        e.preventDefault();
        if ($('#dd_FrequencySip').val() == "0") {
            $('#span_SelectFrequency').removeClass('hide');
        }
        else if ($('#txt_installmentamount').val() == "") {
            $('#span_instlmntamount').removeClass('hide');
        }
        else if ($('#txt_numberofwithdrawls').val() == "") {
            $('#span_withdrawlsno').removeClass('hide');
        }
        else {
            $('.ui-loader').removeClass('hide');
            $.post("/Myaccount/SwpFund", { schemecode: $(this).data('schemcode'), Frequency: $('#dd_FrequencySip').val(), Amount: $('#txt_installmentamount').val(), NumberofWithdrawl: $('#txt_numberofwithdrawls').val() }, function (result) {
                jsonresult = result.split('|');
                $('.ui-loader').addClass('hide');
                if (jsonresult[0] == "Success") {
                    $.confirm({
                        title: 'SWP Successful',
                        content: 'SWP Initiated',
                        buttons: {
                            Done: function () {
                                btnClass: 'btn-blue',
                                    location.href = '/MyAccount/TransactionHistory';
                            },
                            cancel: function () {
                                $.alert('Canceled!');
                            },
                        }
                    });
                }
                if (jsonresult[0] == "Failed") {
                    $.alert({
                        title: 'Alert!',
                        content: jsonresult[1],
                    });
                }
            })
        }
    })
</script>

