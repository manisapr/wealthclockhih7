<script src="@Url.Content("~/js/highcharts.js")"></script>
<script>

    $(document).ready(function () {
        GenarateChart();
        $("#txtamnt").keypress(function (e) {
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                return false;
            }
        });
        $('#txtamnt,#txtage,#txtretmnt,#ddinterval,#ddRisk').change(function () {
            GenarateChart();
        });

        $('.Toolsdiv').click(function () {
            location.href = '@Url.Content("~/MoneyTools")';
        });
        $('.Solutiondiv').click(function () {
            location.href = '@Url.Content("~/Solutions")';
        });
        $('.handpickdiv').click(function () {
            location.href = '@Url.Content("~/WealthclockPicks")';
        });
        $('.insightdiv').click(function () {
            location.href = '@Url.Content("~/Insights")';
        });
    });

    function FromatIndiancurrency(dight) {
        var x = parseInt(dight);
        x = x.toString();
        var lastThree = x.substring(x.length - 3);
        var otherNumbers = x.substring(0, x.length - 3);
        if (otherNumbers != '')
            lastThree = ',' + lastThree;
        var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree;
        return res;
    }


    function GenarateChart() {
        $('#txtage').css({
            "border-color": "#cccccc",
            "border-weight": "1px",
            "border-style": "solid"
        });
        $('#txtretmnt').css({
            "border-color": "#cccccc",
            "border-weight": "1px",
            "border-style": "solid"
        });
        $('#txtamnt').css({
            "border-color": "#cccccc",
            "border-weight": "1px",
            "border-style": "solid"
        });

        if ($('#txtage').val() < 18) {
            alert("Please enter age value greater then 18 years.");
            $('#txtage').focus();
            $('#txtage').css({
                "border-color": "red",
                "border-weight": "1px",
                "border-style": "solid"
            });
            return;
        }

        if ($('#txtamnt').val() == "" || $('#txtamnt').val() < 1000) {
            alert("Please enter minimum Rs 1000.");
            $('#txtamnt').focus();
            $('#txtamnt').css({
                "border-color": "red",
                "border-weight": "1px",
                "border-style": "solid"
            });
            return;
        }

        var amnt = $('#txtamnt').val();
        var d = new Date();
        var n = d.getFullYear();
        var curentyear = n;
        var diffyear = parseInt($('#txtretmnt').val()) - parseInt($('#txtage').val());
        if (diffyear < 5) {
            alert("Need minimum 5 years differance");
            $('#txtretmnt').focus();
            $('#txtretmnt').css({
                "border-color": "red",
                "border-weight": "1px",
                "border-style": "solid"
            });
            return;
        }
        var lastyear = parseInt(curentyear) + parseInt(diffyear);
        var interval = parseInt($('#ddinterval').val()) * parseInt(diffyear);
        var Risk = parseFloat($('#ddRisk').val());
        var interest = (Risk / parseInt($('#ddinterval').val())) / 100;

        var TotalInvst, TotalReturn, TotalInvst1, TotalInvst2, TotalInvst3, TotalInvst4, TotalReturn1, TotalReturn2, TotalReturn3, TotalReturn4;
        var curentyear1, curentyear2, curentyear3, curentyear4;
        var Invst = [], Return = [];

        for (var i = 1; i <= diffyear; i++) {
            interval = parseInt($('#ddinterval').val()) * parseInt(i);
            Invst.push(parseInt($('#txtamnt').val()) * parseInt(interval));
            var returnamnt = (amnt * (((Math.pow(1 + interest, interval) - 1) / interest)) * (1 + interest)).toFixed(2);
            Return.push(parseInt(returnamnt));
            returnamnt = FromatIndiancurrency(returnamnt);
            $('#sntamt').text(returnamnt);
        }


        $('#line-example').html('');

        Highcharts.chart('line-example', {
            chart: {
                type: 'area',
                backgroundColor: '#e6f2f5'
            },
            title: {
                text: ''
            },
            subtitle: {
                text: ''
            },
            xAxis: {
                allowDecimals: false,
                gridLineWidth: 0,
                labels: {
                    formatter: function () {
                        return this.value; // clean, unformatted number for year
                    }
                }
            },
            yAxis: {
                title: {
                    text: ''
                },
                opposite: true,
                gridLineWidth: 0,
                labels: {
                    formatter: function () {
                        return this.value / 1000 + 'k';
                    }
                }
            },
            tooltip: {
                pointFormat: '{series.name} produced <b>{point.y:,.0f}</b><br/>warheads in {point.x}'
            },
            plotOptions: {
                area: {
                    pointStart: curentyear,
                    marker: {
                        enabled: false,
                        symbol: 'circle',
                        radius: 2,
                        states: {
                            hover: {
                                enabled: true
                            }
                        }
                    }
                }
            },
            series: [{
                showInLegend: false,
                name: 'Return',
                data: Return,
                color: '#386BC9'
            }, {
                showInLegend: false,
                name: 'Investment',
                data: Invst,
                color: '#FEB704'
            }]
        });
    }

</script>


























