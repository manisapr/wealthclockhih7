                                        <div class="progress_content">
                                            <h4 id="idGoldAmount">R.S 151856545</h4>
                                            <p><span>Gold</span>7.85% after taxes</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="progress vertical">
                                    <div class="progress-bar progress-bar-info prg_bar_3" id="idSensex" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="max-height: 80%;">
